datasource db {
	provider = "postgresql"
	url      = env("DATABASE_URL")
}

generator client {
	provider = "prisma-client-js"
	previewFeatures = []
}

model Phantom {
	id Int @id @default(autoincrement())
	@@ignore
}

model ThingA {
	id Int @id @default(dbgenerated("get_next_phantom_id()"))
}

model ThingB {
	id Int @id @default(dbgenerated("get_next_phantom_id()"))
}